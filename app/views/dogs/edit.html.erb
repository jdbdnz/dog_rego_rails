<h2>Edit <%= @dog.name %></h2>

<%= bootstrap_form_for(@dog, url: user_dog_path, html: {  method: :put }) do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :breed %>
  <%= f.input :date_of_birth, as: :date, start_year: (DateTime.current.year - 50), end_year: DateTime.current.year %>
  <%= f.button :submit, "Update", class: "col-xs-12 btn btn-success" %>
<% end %>
